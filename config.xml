<?xml version='1.0' encoding='utf-8'?>
<widget id="visualizador.map4rdfmobile" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Map4RDF Mobile</name>
    <description>
        Aplaci√≥n de visualizacion de datos geograficos en dispositivo movil.
    </description>
    <author email="diego.camacho.ballesteros@alumnos.upm.es" href="http://www.fi.upm.es">
        Diego Camacho Ballesteros
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="www/img/icono.png"/>
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1" />
    <engine name="android" spec="^7.1.1" />
</widget>
